<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <LangVersion>preview</LangVersion>
        <StartupObject>Volte.Program</StartupObject>
        <RuntimeIdentifiers>linux-x64</RuntimeIdentifiers>
        <RuntimeIdentifiers>win10-x64</RuntimeIdentifiers>
        <Authors>GreemDev</Authors>
        <Company>Ultz</Company>
        <PackageProjectUrl>https://greemdev.net/Volte</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Ultz/Volte</RepositoryUrl>
        <Copyright>MIT</Copyright>
        <Version>4.0.0.0</Version>
        <Platforms>AnyCPU;x64</Platforms>
        <ApplicationIcon>Volte.ico</ApplicationIcon>
        <PlatformTarget>x64</PlatformTarget>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\.gitignore" Link=".gitignore" />
        <None Include="..\data\volte.json" Link="config" />
        <None Include="..\.editorconfig" Link=".editorconfig" />
        <None Include="..\LICENSE">
            <Pack>True</Pack>
        </None>
        <PackageReference Include="Colorful.Console" Version="1.2.11" />
        <PackageReference Include="DSharpPlus" Version="4.0.0-nightly-00721" />
        <PackageReference Include="DSharpPlus.Interactivity" Version="4.0.0-nightly-00721" />
        <PackageReference Include="Gommon" Version="1.2.2" />
        <PackageReference Include="Humanizer.Core" Version="2.8.26" />
        <PackageReference Include="JetBrains.Annotations" Version="2020.1.0" />
        <PackageReference Include="LiteDB" Version="5.0.9" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.7.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.0-preview.8.20407.11" />
        <PackageReference Include="Qmmands" Version="3.1.3-nightly-00248" />
        <PackageReference Include="RestSharp" Version="106.11.5-alpha.0.18" />
        <PackageReference Include="SixLabors.ImageSharp" Version="1.0.1" />
    </ItemGroup>
</Project>